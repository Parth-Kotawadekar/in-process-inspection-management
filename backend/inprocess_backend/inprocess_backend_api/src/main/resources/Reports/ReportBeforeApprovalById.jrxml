    datasheet_tb.ref_dc_no,
    datasheet_tb.date_of_reciept,  -- corrected from "date_of_reciept"
    datasheet_tb.issue_date,
    datasheet_tb.certificate_no,
    datasheet_tb.calibration_place,
    datasheet_tb.identification_marked_by,
    employee_tb.emp_id,
    employee_tb.first_name,
    employee_tb.last_name,
    employee_tb.designation,
    employee_tb.signature,
    customer_tb.cust_id,
    customer_tb.customer_name,
    customer_tb.adderss,
    customer_tb.logo,
    combined_master_1_tb.rep_id,
    combined_master_1_tb.rep_sr_no,
    combined_master_1_tb.condition_description,
    combined_master_1_tb.test_purpose,
